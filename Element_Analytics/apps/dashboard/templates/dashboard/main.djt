<!DOCTYPE html>
<html>
{% extends "userbase.djt" %}

{% block title %}
<title>Dashboard</title>
{% endblock %}

{% block section1 %}
<h1>Howdy, {{ user.username}}</h1>
{% endblock %}

{% block section2 %}
     <p>Your files are here:</p>
     {% for file in file_list %}
     {% if file != ".DS_Store" %}
       {# <p><a href="{% url 'analytics:file_home' file %}">{{ file }}</a></p> #}
      <p><a href="/media/document/{{ file }}">{{ file }}</a></p>
     {% endif %}
     {% endfor %}
 {% endblock %}

</html>
